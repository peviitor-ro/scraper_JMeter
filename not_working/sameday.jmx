var city =  &apos;&quot;Bucure»ôti&quot;&apos;;
if(title.contains(&quot;Ilfov&quot;)){
	city = &apos;&quot;Ilfov&quot;&apos;;
}
if(title.contains(&quot;Bucure»ôti/Ilfov&quot;)){
	city = &apos;[&quot;Bucure»ôti&quot;,&quot;Ilfov&quot;]&apos;;
}
if(title.contains(&quot;Sibiu&quot;)){
	city = &apos;&quot;Sibiu&quot;&apos;;
