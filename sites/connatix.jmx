                <stringProp name="script">var item = vars.get(&quot;k&quot;)
String location = vars.get(&quot;location_&quot;+item)
String[] loc = location.split(&quot;,&quot;);
vars.put(&quot;town&quot;,loc[0]);
//If we have only the city
if(loc.size() != 2){
	if(loc[0].equals(&quot;Cluj-Napoca&quot;)){
		vars.put(&quot;country&quot;,&quot;România&quot;);
	}
	if(loc[0].equals(&quot;London&quot;)){
		vars.put(&quot;country&quot;,&quot;United Kingdom&quot;);
	}
	if(loc[0].equals(&quot;New York&quot;)){
		vars.put(&quot;country&quot;,&quot;United States&quot;);
	}
}
else{
