var city=vars.get(&quot;cities_&quot; + k).replace(&quot;Bucharest&quot;,&quot;București&quot;).replace(&quot;Cluj&quot;,&quot;Cluj-Napoca&quot;).replace(&quot;Timisoara&quot;,&quot;Timișoara&quot;).replace(&quot;Targu&quot;,&quot;Târgu Mureș&quot;).replace(&quot;Mosnita&quot;,&quot;Moșnița Nouă&quot;);
