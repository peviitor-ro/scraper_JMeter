              <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Setting up variables to get jobs for ${location}" enabled="true">
                <stringProp name="scriptLanguage">groovy</stringProp>
                <stringProp name="parameters"></stringProp>
                <stringProp name="filename"></stringProp>
                <stringProp name="cacheKey">true</stringProp>
                <stringProp name="script">import java.net.URLEncoder;
var k = vars.get(&quot;k&quot;);
var location = vars.get(&quot;locations_&quot;+k)
var jobcount = vars.get(&quot;jobcount_&quot;+k)
var count = Integer.valueOf(jobcount.split(&quot; &quot;)[0])
var loc = URLEncoder.encode(location.replace(&quot;Romania&quot;,&quot;RO&quot;),&quot;UTF-8&quot;)
log.info(loc)
vars.put(&quot;loc&quot;,loc)
vars.putObject(&quot;k2&quot;,0)
vars.putObject(&quot;count&quot;,count/10+2)</stringProp>
              </JSR223Sampler>
