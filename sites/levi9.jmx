var title = vars.get(&quot;title_&quot;+k);
var link  = vars.get(&quot;link_&quot;+k);
var country = vars.get(&quot;country&quot;);
var city =  vars.get(&quot;city_&quot;+k).replace(&quot;Iasi&quot;,&quot;Ia»ôi&quot;);

String this_job

